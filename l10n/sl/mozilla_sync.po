msgstr "Shranjeni podatki so izbrisani"

#: ajax/deletestorage.php:41
msgid "User not found"
msgstr "Uporabnika ni mogoče najti"

#: ajax/restrictgroup.php:32
msgid "Restriction saved"
msgstr "Omejitev je shranjena"

#: ajax/setemail.php:24 ajax/setquota.php:24
msgid "Invalid input"
msgstr "Neveljaven vnos"

#: ajax/setemail.php:30
